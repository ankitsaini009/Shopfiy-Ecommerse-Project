  document.addEventListener("DOMContentLoaded", function () {
    const updateWishlistCount = () => {
      // Get wishlist data from localStorage
      const wishlist = JSON.parse(localStorage.getItem("wishlist")) || [];
      // Update the wishlist count in the header
      const wishlistCountElement = document.getElementById("wishlist-count");
      if (wishlistCountElement) {
        wishlistCountElement.textContent = wishlist.length;
      }
    };

    // Initialize the wishlist count on page load
    updateWishlistCount();

    // Attach click event listeners to wishlist buttons
    const wishlistBtns = document.querySelectorAll(".wishlist-btn");
    wishlistBtns.forEach((btn) => {
      btn.addEventListener("click", function () {
        // Simulate a delay to update the count after an action
        setTimeout(updateWishlistCount, 100);
      });
    });
  });

    document.addEventListener("DOMContentLoaded", function () {
    const updateComparisonCount = () => {
      // Get comparison list data from localStorage
      const comparisonList = JSON.parse(localStorage.getItem("comparisonList")) || [];
      
      // Update the comparison count in the header
      const comparisonCountElement = document.getElementById("comparison-count");
      if (comparisonCountElement) {
        comparisonCountElement.textContent = comparisonList.length;
      }
    };

    // Initialize the comparison count on page load
    updateComparisonCount();

